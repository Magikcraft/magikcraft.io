{
  "name": "Magikcraft API Documentation",
  "mode": "file",
  "out": "doc",
  "target": "ES5",
  "ignoreCompilerErrors": "false",
  "excludePrivate": "true",
  "preserveConstEnums": "true",
  "includes": "./typings/index.d.ts",
  "excludeExternals": "true",
  "exclude": "node_modules/@types/*",
  "includeDeclarations": "true",
  "external-modulemap": ".*\/modules\/@mycompany\/([\\w\\-_]+)\/",
  "stripInternal": "false",
  "theme": "node_modules/typedoc-dash-theme"
}